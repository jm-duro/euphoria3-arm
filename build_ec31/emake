echo linking
gcc ec.o main-.o main-0.o misc.o wildcard.o global.o error.o machine.o c_out.o symtab.o symtab_0.o scanner.o scanne_0.o file.o sort.o emit.o emit_0.o emit_1.o parser.o parser_0.o parser_1.o c_decl.o c_dec0.o c_dec1.o compile.o compil_0.o compil_1.o compil_2.o compil_3.o compil_4.o compil_5.o compil_6.o compil_7.o compil_8.o compil_9.o traninit.o get.o main.o pathopen.o init-.o -l:ecu.a -lm -ldl -pthread -oecu
echo you can now execute: ./ecu
